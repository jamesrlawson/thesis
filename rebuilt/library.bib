journal = {Annals of Botany},
keywords = {Allometry,Community ecology,Diameter,Functional traits,Height,Saplings,Stem volume,Trait correlations,Wood density},
pages = {987--993},
pmid = {19635742},
title = {{Costs of height gain in rainforest saplings: Main-stem scaling, functional traits and strategy variation across 75 species}},
