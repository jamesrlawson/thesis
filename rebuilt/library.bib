journal = {Ecology Letters},
keywords = {Functional diversity,Functional turnover,Global change,Metacommunities,Multifunctionality,Spatial and temporal heterogeneity,Species richness,Species traits},
number = {12},
pages = {1405--1419},
pmid = {19849711},
title = {{Biodiversity in a complex world: consolidation and progress in functional biodiversity research}},
