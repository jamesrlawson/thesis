journal = {New Phytologist},
keywords = {Carbon,Ecosystem,Elevated CO2,Growth,Nutrients,Productivity,Soil},
pages = {393--411},
pmid = {17083672},
title = {{Plant CO2 responses: An issue of definition, time and resource supply}},
volume = {172},
year = {2006}
}
