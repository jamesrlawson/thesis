title = {{Reactive oxygen species mediate growth and death in submerged plants.}},
url = {http://www.pubmedcentral.nih.gov/articlerender.fcgi?artid=3671184{\&}tool=pmcentrez{\&}rendertype=abstract},
volume = {4},
