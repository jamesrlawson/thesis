journal = {Annals of botany},
keywords = {Biological Evolution,Ecology,Ecosystem,Fruit,Fruit: anatomy {\&} histology,Multivariate Analysis,Phylogeny,Plant Leaves,Plant Leaves: anatomy {\&} histology,Plants,Plants: anatomy {\&} histology,Seeds,Seeds: anatomy {\&} histology,Trees,Tropical Climate,wood density},
