doi = {10.1146/annurev.arplant.48.1.223},
file = {:C$\backslash$:/Users/James/Desktop/stuff/articles/drew 1997{\_}oxygen deficiency and root metabolism - injury and acclimation under hypoxia and anoxia.pdf:pdf},
isbn = {1040-2519},
issn = {1040-2519},
journal = {Annual Review of Plant Physiology and Plant Molecular Biology},
keywords = {aerenchyma,anaerobic,fermentation,metabolism,programmed cell death},
