journal = {Ecology},
keywords = {Ecosystem,Models, Biological,Plant Components, Aerial,Plant Components, Aerial: genetics,Plants,Plants: genetics,Quantitative Trait, Heritable,wood density},
mendeley-tags = {wood density},
month = {jun},
