@article{Ryser2011,
abstract = {To understand the economics of root aerenchyma formation in wetland$\backslash$nplants, we investigated in detail the response of Alisma triviale$\backslash$nto waterlogging. We hypothesized costs being associated with development$\backslash$nof a large root air space. In three out-door pot experiments, seedlings$\backslash$n(1 experiment) and mature plants (2 experiments) were grown under$\backslash$nwaterlogged and drained conditions for up to 2 months. Waterlogging$\backslash$npromoted growth, and was associated with increased root porosity$\backslash$nand decreased root density (fresh mass per volume). The increased$\backslash$nformation of aerenchyma was associated with a higher root dry matter$\backslash$ncontent for a given root density. Despite improved growth and earlier$\backslash$nflowering, the waterlogged plants also showed signs of being constrained$\backslash$nby the anoxic substrate, such as shallower roots, and a higher leaf$\backslash$ndry matter content. The formation of aerenchyma was associated with$\backslash$ncosts, such as increased root dry matter content and reduced metaxylem$\backslash$nvessel diameter. The faster growth of the seedlings under the waterlogged$\backslash$nconditions, despite some signs of being stressed, was possibly a$\backslash$nresult of decreased requirements to allocate biomass below ground.$\backslash$nIn mature plants the increased aerenchyma allowed deeper root penetration,$\backslash$nand ameliorated the effects of anoxia, reducing the differences in$\backslash$nplant traits between the treatments.},
author = {Ryser, Peter and Gill, Harneet K. and Byrne, Collin J.},
doi = {10.1007/s11104-011-0715-0},
file = {:C$\backslash$:/Users/James/Desktop/stuff/articles/ryser et al 2011{\_}constraints of root response to waterlogging in alisma triviale.pdf:pdf},
isbn = {1110401107150},
issn = {0032079X},
journal = {Plant and Soil},
keywords = {Aerenchyma,Alisma triviale,Biomass allocation,Economics,Root anatomy,Root dry matter content,Waterlogging},
pages = {247--260},
title = {{Constraints of root response to waterlogging in Alisma triviale}},
volume = {343},
year = {2011}
}
